{
  "core-plugins": [
    "file-explorer",
    "search",
    "switcher",
    "graph",
    "backlink",
    "page-preview",
    "command-palette",
    "markdown-importer",
    "word-count",
    "templates",
    "daily-notes"
  ],
  "foldHeading": true,
  "foldIndent": true,
